{
    "name": "ft_job_logger",
    "productName": "FT Job Logger",
    "version": "2.2.9",
    "description": "Desktop App for the Falcon Trucking Tracker",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "dist": "electron-builder",
        "release": "electron-builder -p always",
        "checkupdates": "ncu -u",
        "postinstall": "electron-builder install-app-deps"
    },
    "config": {
        "pass": "ftjobloggerprasadhdesktop"
    },
    "build": {
        "appId": "FtJobLogger",
        "win": {
            "target": "NSIS",
            "icon": "./assets/falcon_icon.ico",
            "asar": true
        },
        "publish": {
            "provider": "generic",
            "url": "https://falconites.com/dashboard/cdn/releases"
        },
        "nsis": {
            "artifactName": "${productName} Setup ${version}.${ext}",
            "include": "build/installer.nsh"
        }
    },
    "author": "Falcon_Trucking",
    "license": "ISC",
    "dependencies": {
        "axios": "^0.19.0",
        "crypto-js": "^3.1.9-1",
        "discord-rpc": "^3.1.0",
        "electron-log": "^4.0.0",
        "electron-updater": "^4.2.0",
        "is-online": "^8.2.1",
        "node-fetch": "^2.6.0"
    },
    "devDependencies": {
        "discord-rpc": "^3.1.0",
        "electron": "^7.1.4",
        "electron-builder": "^22.2.0",
        "electron-prebuilt": "^1.4.13",
        "electron-updater": "^4.2.0",
        "nexe": "^3.3.2",
        "npm-check-updates": "^4.0.1",
        "node-fetch": "^2.6.0"
    }
}